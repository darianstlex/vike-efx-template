{
	"scripts": {
		"server": "node --import './server/register.js' ./server/index.ts",
		"server:dev": "npm run server --force",
		"dev": "npm run server:dev",
		"start": "npm run server:dev",
		"func": "cross-env TZ=UTC npx cypress run --e2e -b chrome --headless",
		"func:open": "cross-env TZ=UTC npx cypress open --e2e -b chrome",
		"func:coverage": "cross-env CYPRESS_COVERAGE=true start-server-and-test server:dev http-get://localhost:3000 func",
		"unit": "cross-env TZ=UTC npx cypress run --component -b chrome --headless",
		"unit:open": "cross-env TZ=UTC npx cypress open --component -b chrome",
		"unit:coverage": "cross-env TZ=UTC CYPRESS_COVERAGE=true npx cypress run --component -b chrome --headless",
		"test:coverage": "rm -rf .nyc_output && npm run unit:coverage && npm run func:coverage",
		"lint": "eslint . --max-warnings 0",
		"build": "vite build",
		"server:prod": "cross-env NODE_ENV=production npm run server",
		"prod": "npm run lint && npm run build && npm run server:prod",
		"prepare": "husky"
	},
	"dependencies": {
		"classnames": "^2.5.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"effector": "^23.2.2",
		"effector-react": "^23.2.1",
		"express": "^4.19.2",
		"lodash-es": "^4.17.21",
		"patronum": "^2.2.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-use": "^17.5.0",
		"sirv": "^2.0.4",
		"telefunc": "^0.1.76",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@mdx-js/rollup": "^3.0.1",
		"@svgr/rollup": "^8.1.0",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/istanbul-lib-coverage": "^2.0.6",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"cross-env": "^7.0.3",
		"cypress": "^13.13.0",
		"cypress-vite": "^1.5.0",
		"dotenv": "^16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.2",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sort-destructure-keys": "^2.0.0",
		"eslint-plugin-typescript-sort-keys": "^3.2.0",
		"husky": "^9.0.11",
		"istanbul-lib-coverage": "^3.2.2",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.2",
		"prettier-eslint": "^16.3.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.77.6",
		"start-server-and-test": "^2.0.4",
		"typescript": "^5.5.3",
		"vike": "^0.4.178",
		"vite": "^5.3.3",
		"vite-plugin-istanbul": "^6.0.2"
	},
	"engines": {
		"node": "^20",
		"npm": "^10"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"eslint --fix",
			"prettier --write --ignore-unknown"
		]
	},
	"type": "module"
}
